{
  "description": "all modelled properties of a user",
  "type": "object",
  "properties": {
    "firstName": {
      "type": "string",
      "isNotEmpty": true,
      "description": "first name of the user"
    },
    "lastName": {
      "type": "string",
      "isNotEmpty": true,
      "description": "last name of the user"
    },
    "email": {
      "type": "string",
      "format": "email",
      "description": "email of the user"
    },
    "password": {
      "type": "string",
      "isNotEmpty": true,
      "description": "password of the user"
    }
  },
  "required": ["firstName", "lastName", "email", "password"]
}
